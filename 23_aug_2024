odd or even
public class Odd {
    public static void main(String[] args) {
        for(int i=0;i<=10;i++){
            if(i%2==0){
                System.out.println("even");
            }
            else{
                System.out.println("odd");
            }
        }
    }
}
postive or negative
import java.util.Scanner;

public class Positive {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int num=sc.nextInt();
        if(num>0){
            System.out.print("+ive");
        } else if ((num<0)) {
            System.out.print("+ive");
        }
        else{
            System.out.print("Zero");
        }
    }
}
